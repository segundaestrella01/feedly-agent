{
  "name": "rss-agent",
  "version": "1.0.0",
  "type": "module",
  "description": "RSS content aggregator and summarizer: RAG-based retrieval, clustering, and daily digest generation.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "fetch": "tsx src/workers/fetcher.ts",
    "chunk": "tsx src/workers/chunker.ts",
    "process": "tsx src/workers/processor.ts",
    "digest": "tsx src/workers/digestGenerator.ts",
    "test": "jest",
    "test:embedding": "tsx src/scripts/testEmbedding.ts",
    "test:chroma": "tsx src/scripts/testChroma.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "rag",
    "ai",
    "rss",
    "summarizer",
    "embeddings",
    "vector-database",
    "typescript",
    "nodejs"
  ],
  "author": "segundaestrella01",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.1",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/segundaestrella01/feedly-agent.git"
  },
  "bugs": {
    "url": "https://github.com/segundaestrella01/feedly-agent/issues"
  },
  "homepage": "https://github.com/segundaestrella01/feedly-agent#readme",
  "dependencies": {
    "@types/jsdom": "^27.0.0",
    "chromadb": "^3.1.6",
    "dotenv": "^17.2.3",
    "jsdom": "^27.2.0",
    "openai": "^6.9.1",
    "rss-parser": "^3.13.0"
  }
}
